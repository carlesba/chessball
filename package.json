    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "reset-css": "^4.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.2.2",
    "parcel-bundler": "^1.12.3",
    "react-testing-library": "^7.0.0",
