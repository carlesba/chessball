{
  "name": "chessball",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel src/index.html --out-dir public",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlesba/chessball.git"
  },
  "author": "carlesba",
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
