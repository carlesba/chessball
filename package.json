    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.css$": "<rootDir>/src/styles.css.d.ts"
    },
    "testMatch": [
      "**/src/**/*.(test|spec).(ts|tsx|js)"
    ]
  },
